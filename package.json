    "@emotion/styled": "^11",
    "@fontsource/montserrat": "^4.4.5",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "framer-motion": "^4",
    "gatsby": "^5.13.1",
    "gatsby-image": "^3.3.0",
    "gatsby-plugin-catch-links": "^3.3.0",
    "gatsby-plugin-gdpr-cookies": "^2.0.0",
    "gatsby-plugin-google-gtag": "^3.3.0",
    "gatsby-plugin-image": "3.13.0",
    "gatsby-plugin-lodash": "^4.3.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-source-prismic": "6.0.1",
    "gatsby-transformer-sharp": "^5.13.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "prismic-reactjs": "^1.3.4",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-headroom": "^3.1.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-typography": "^0.16.19",
    "ts-node": "^10.0.0",
    "typeface-montserrat": "^1.1.13",
    "typeface-nunito": "^1.1.13",
    "typography": "^0.16.19"
